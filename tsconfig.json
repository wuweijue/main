{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "baseUrl": "./",
        "paths": {
            "useTranslate": [
                "./src/utils/useTranslate"
            ],
            "@utils/*": [
                "./src/utils/*"
            ],
            "@store/*": [
                "./src/store/*"
            ]
        }
    } 
}